{
	"html": {
		"collapseBooleanAttributes": true,
		"collapseWhitespace": true,
		"decodeEntities": true,
		"minifyCSS": true,
		"minifyJS": true,
		"removeAttributeQuotes": false,
		"sortAttributes": true,
		"sortClassName": true
	},
	"css": {
		"type": "clean-css",
		"clean-css": {
			"level": 2
		}
	}
}
